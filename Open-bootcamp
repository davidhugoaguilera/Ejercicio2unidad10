from tkinter import *

master = Tk()

elemento = StringVar()

listbox = Listbox(master)

for item in ["Lio Messi", "Angelito Di Mar√≠a", "Rodri De Paul", "Nico Otamendi", "Enzo Fernandez", "Dibu Martinez", "Cuti Romero", "Julian Alvarez"]:
    listbox.insert(END, item)

